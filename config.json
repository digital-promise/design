{
  "source": ["./tokens/**/*.json"],
  "platforms": {
      "ts": {
          "transformGroup": "js",
          "buildPath": "./dist/",
          "files": [
              {
                  "destination": "tokens.js",
                  "format": "javascript/module"
              },
              {
                  "format": "typescript/module-declarations",
                  "destination": "tokens.d.ts"
                }
          ]
      }
  }
}